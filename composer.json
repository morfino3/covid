{
    "name": "laboratory/covid",
    "description": "Laravel wrapper of php-ffmpeg for simple video conversion, editing, resizing etc., utilizing FFMpeg's powerful open source library/tool",
    "type": "library",
    "authors": [
       {
           "name": "Mike Ambait",
           "email": "michaelambait16@gmail.com"
       }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/config": "^6.0",
        "illuminate/filesystem": "^6.0",
        "illuminate/log": "^6.0",
        "illuminate/support": "^6.0",
        "league/flysystem": "^1.0.8",
        "pbmedia/php-ffmpeg": "^0.14.1",
        "symfony/process": "^4.3.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "phpunit/phpunit": "^8.3",
        "twistor/flysystem-http": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Laboratory\\Covid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laboratory\\Covid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laboratory\\Covid\\FFMpegServiceProvider"
            ],
            "aliases": {
                "Covid": "Laboratory\\Covid\\Facades\\Covid"
            }
        }
    }
}